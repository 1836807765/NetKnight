<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pencilbox.netknight">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/knight"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.DemoAty" />
        <activity android:name=".view.MainApp" />
        <activity android:name=".view.MainAddress" />
        <activity android:name=".view.MainIp" />
        <activity android:name=".view.MainPrefer" />
        <activity android:name=".view.GraspBag" />

        <activity
            android:name=".view.DairyTabbed"
            android:label="@string/title_activity_dairy_tabbed"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".view.MainTabbed"
            android:label="@string/appname"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>